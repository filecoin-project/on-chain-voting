{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@web3-storage/w3up-client": ["node_modules/@web3-storage/w3up-client"],
      "@tanstack/react-query": ["node_modules/@tanstack/react-query"],
    },
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
  },
  "exclude": [
    "node_modules",
  ]
}
